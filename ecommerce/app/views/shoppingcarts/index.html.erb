<h1>Listing shoppingcarts</h1>

<table>
  <tr>
    <th>Items</th>
    <th>Owner</th>
  </tr>

<% @shoppingcarts.each do |shoppingcart| %>
  <tr>
    <td><%=h shoppingcart.items %></td>
    <td><%=h shoppingcart.owner %></td>
    <td><%= link_to 'Show', shoppingcart %></td>
    <td><%= link_to 'Edit', edit_shoppingcart_path(shoppingcart) %></td>
    <td><%= link_to 'Destroy', shoppingcart, :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New shoppingcart', new_shoppingcart_path %>