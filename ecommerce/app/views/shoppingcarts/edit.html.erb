<h1>Editing shoppingcart</h1>

<% form_for(@shoppingcart) do |f| %>
  <%= f.error_messages %>

  <p>
    <%= f.label :items %><br />
    <%= f.text_field :items %>
  </p>
  <p>
    <%= f.label :owner %><br />
    <%= f.text_field :owner %>
  </p>
  <p>
    <%= f.submit 'Update' %>
  </p>
<% end %>

<%= link_to 'Show', @shoppingcart %> |
<%= link_to 'Back', shoppingcarts_path %>